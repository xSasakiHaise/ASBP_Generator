<div class="row">
    <div class="col s12">
        <div class="card blue lighten-5">
            <div class = "card-content">
            <span class="card-title">Level {{index}} Rewards</span>
            {{#each rewards}}
            {{#if this.rewards.length}}
            <div class="indigo lighten-5 track-output">
                <span><strong>{{#if this.label}}{{this.label}}{{else}}{{#if this.display.name}}{{this.display.name}}{{else}}{{@key}}{{/if}}{{/if}}:</strong></span>
                {{#if this.permission}}
                <p>Permission: {{this.permission}}</p>
                {{/if}}
                <p>Lore: {{this.display.lore}}</p>
                <p>Display Item: {{this.display.item}}</p>
                <p>Commands to Run: {{this.rewards}}</p>
            </div>
            {{/if}}
            {{/each}}
            </div>
        </div>
    </div>
</div>